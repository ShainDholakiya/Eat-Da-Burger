{{> header }}

<div class="main-devoured">
    <h2>All your devoured burgers</h2>
   <div class="main-devoured__burgers">
       {{#each burgers}} 

       <div class="content-burger_list" id={{id}}>
        <img src="https://images.unsplash.com/photo-1551615593-ef5fe247e8f7?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=1680&q=80" alt="burger">
            <p>{{burger_name}}</p>
            <button class="btn btn-outline-dark devoured" data-id="{{id}}" data-state="{{devoured}}">
                {{#if devoured}} Devoured {{else}} Devour it! {{/if}}
            </button>
        </div>

       {{/each}} 
   </div>
</div>